{
	"name": "eGauge - Grafana",
	"dockerComposeFile": "docker-compose.yml",
	"service": "dev",
	"workspaceFolder": "/workspace",
	"postStartCommand": "pip install --upgrade pip",
	"remoteUser": "dev",
	"customizations": {
		"vscodium": {
			"registries": {
				"openvsx": {
					"url": "https://open-vsx.org/vscode/gallery",
					"headers": {
						"Accept": "application/json"
					}
				}
			},
			"extensions": {
				"ms-python.debugpy": {},
				"ms-python.vscode-python-envs": {},
        		"ms-python.python": {}
			}
		}
	}
}